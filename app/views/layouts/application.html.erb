<!DOCTYPE html>
<html>
  <head>
    <title>RakurakuRenrakuchou</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if parent_signed_in? %>
      <li>
        <%= link_to "ログアウト", destroy_parent_session_path, methot: :delete %>
      </li>
    <% else %>
      <li>
        <%= link_to "新規登録", new_parent_registration_path %>
      </li>
      <li>
        <%= link_to "ログイン", new_parent_session_path %>
      </li>
    <% end %>
    <%= yield %>
  </body>
</html>
